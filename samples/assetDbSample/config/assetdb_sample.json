{
  "app": {
    "name": "AssetDbSample",
    "author": "Nau Engine",
    "version": "0.0.1",
    "log": {
      "files": [
        "$folder{LocalAppData}/nau/${app/name}/logs/log"
      ]
    },
    "vfs": {
      "mounts": [
        {
          "mountPoint": "/content",
          "path": "$sampleDir{sceneBase}/content",
          "readOnly": true
        },
		{
          "mountPoint": "/res",
          "path": "$sampleDir{sceneBase}/resources",
          "readOnly": true
        },
        {
          "mountPoint": "/assets_db",
          "path": "${sampleProjectDir}/assets_database",
          "readOnly": true
        }
      ]
    }
  }
}