{
  "app": {
    "name": "UiSample",
    "author": "Nau Engine",
    "version": "0.0.1",
    "log": {
      "files": [
        "$folder{LocalAppData}/nau/${app/name}/logs/log"
      ]
    },
    "vfs": {
      "mounts": [
        {
          "mountPoint": "/cocos",
          "path": "${sampleProjectDir}/../../engine/core/modules/ui/src/shaders",
          "readOnly": true
        },

        {
          "mountPoint": "/content",
          "path": "$sampleDir{sceneBase}/content",
          "readOnly": true
        },

        {
          "mountPoint": "/res",
          "path": "${sampleProjectDir}/Resources",
          "readOnly": true
        },

        {
          "mountPoint": "/content_ui",
          "path": "${sampleProjectDir}/content",
          "readOnly": true
        }
      ]
    }
  }
}